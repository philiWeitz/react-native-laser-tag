{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "padded-blocks": [0,1],
    "class-methods-use-this": 0,
    "jsx-a11y/href-no-hash": "off",
    "arrow-body-style": [0, "always"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0, {}]
  }
}